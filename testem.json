{
  "framework": "mocha",
  "serve_files": [ "test/browserified.js" ],
  "src_files": [
    "src/*.js",
    "test/*_tests.js"
  ],
  "launchers": {
  },
  "before_tests": "browserify --debug test/*_tests.js -o test/browserified.js",
  "on_exit": "rm test/browserified.js",
  "launch_in_dev": ["Chrome"]
}